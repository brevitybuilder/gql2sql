SELECT json_build_object('session', (SELECT to_json((SELECT "root" FROM (SELECT "base"."sessionToken", "base"."userId", "base"."expires", "user" AS "user2") AS "root")) AS "root" FROM (SELECT * FROM "auth"."sessions" WHERE "sessionToken" = $1 LIMIT 1) AS "base" LEFT JOIN LATERAL (SELECT to_json((SELECT "root" FROM (SELECT "base.users"."id", "base.users"."id", "base.users"."name", "base.users"."email", "base.users"."emailVerified", "base.users"."image") AS "root")) AS "user" FROM (SELECT * FROM "auth"."users" WHERE "auth"."users"."id" = "base"."userId" LIMIT 1) AS "base.users") AS "root.users" ON ('true'))) AS "data"
