SELECT jsonb_build_object('record', (SELECT coalesce(json_agg(to_json((SELECT "root" FROM (SELECT "base"."id", "base"."name", "base"."age") AS "root"))), '[]') AS "root" FROM (SELECT * FROM "Record" WHERE "id" = $1::text) AS "base")) AS "data"
